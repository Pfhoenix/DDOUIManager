<Window x:Class="DDOUIManager.DecompressFileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DDOUIManager"
        mc:Ignorable="d"
        Height="30" Width="200" WindowStyle="None" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
	<Border BorderThickness="1" BorderBrush="Black">
		<DockPanel>
			<Label>Decompressing</Label>
			<ProgressBar DockPanel.Dock="Right" IsIndeterminate="True" Margin="3"/>
		</DockPanel>
	</Border>
</Window>
